# Environment for machines (used by taskfiles/machines.yaml)
WN_CPUS: 2
WN_RAM: 4
WN_DISK: 8


# Environment for Kubernetes, used in following files which are generated from templates
# - machines/ubuntu-lts-machine.yaml
# - manifests/kubadm/cp-1-init-cfg.yaml
KUBERNETES_REPO: "1.33"
KUBERNETES_VERSION: "1.33.3"

# Environment for Ciliun CNI (used by taskfiles/kubernetes.yaml)
GATEWAYAPI_VERSION: "1.2.1"
CILIUM_CHART_VERSION: "1.17.6"

# Environment for local-path-provisioner CSI (used by taskfiles/kubernetes.yaml)
LOCAL_PATH_PROVISIONER_VERSION: "0.0.31"

# Environment for cert-manager addon (used by taskfiles/addons.yaml)
CERT_MANAGER_CHART_VERSION: "1.18.2"

# Environment for metrics-server addon (used by taskfiles/addons.yaml)
METRICS_SERVER_CHART_VERSION: "3.12.2"
